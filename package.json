{
  "name": "japanese-learning-capture",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "backendSrc/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start-without-gui-build": "start /b vite && set SERVER=on&& electron .",
    "app-build": "electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios-https-proxy-fix": "^0.17.1",
    "cheerio": "^1.0.0-rc.12",
    "chinese-simple2traditional": "^2.2.2",
    "electron-store": "^10.0.0",
    "element-plus": "^2.7.7",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "sharp": "^0.33.4",
    "tencentcloud-sdk-nodejs-tmt": "^4.0.890",
    "tslib": "^2.6.3",
    "vue": "^3.4.33",
    "ws": "^8.18.0",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "vite": "^5.2.0"
  },
  "build": {
    "appId": "nya.yoyokity.JapLC",
    "productName": "JapLC",
    "copyright": "Copyright Â© 2022 yoyokity",
    "win": {
      "icon": "resources/icon.ico"
    },
    "extraFiles": [
      "tools"
    ],
    "files": [
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "font.ttf"
        ]
      },
      "!**/*",
      "node_modules",
      "package.json",
      "LICENSE",
      "backendSrc",
      "guiSrc",
      "yo-electron-lib"
    ]
